{
    "plugins": [
        "react"
    ],
    "extends": [
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env":{
        "worker": true,
        "commonjs": true,
        "amd": true
    },
    "rules": {
        "no-duplicate-case": 2,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-parens": [1,"all"],
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-sparse-arrays": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-typeof": 1,
        "no-unexpected-multiline": 2,
        "accessor-pairs": [1,{"getWithoutSet":false,"setWithoutGet":true}],
        "block-scoped-var": 1,
        "consistent-return": 2,
        "curly": [1,"all"],
        "default-case": 1,
        "dot-location": [1,"property"],
        "eqeqeq": [1,"smart"],
        "guard-for-in": 1,
        "no-alert": 1,
        "no-else-return": 1,
        "no-labels": 2,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-fallthrough": 1,
        "no-octal": 1,
        "no-redeclare": [2,{"builtinGlobals":true}],
        "no-label-var": 2,
        "no-undef": 1,
        "no-unused-vars": 1,
        "camelcase": [1,{"properties":"always"}],
        "func-names": 1,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": [1,"smart-tabs"],
        "no-trailing-spaces": [1,{"skipBlankLines":true}],
        "no-underscore-dangle": 1,
        "semi": [1,"always"],
        "space-infix-ops": [1,{"int32Hint":true}],
        "arrow-parens": [1,"as-needed"],
        "constructor-super": 1,
        "no-var": 1,
        "prefer-const": 1,
        "prefer-spread": 1,
        "require-yield": 1,
        "max-statements": [1,40],
        "vars-on-top":1,
        "object-property-newline":1
    }
}